    <beans:bean id="requestContextFilter" class="org.springframework.web.filter.RequestContextFilter">
    	<custom-filter position="FIRST"/>
    </beans:bean>
    
    <authentication-provider>
		<user-service>
	    	<user name="jeremy" password="atlanta" authorities="ROLE_USER, ROLE_ADMIN" />
	      	<user name="keith" password="melbourne" authorities="ROLE_USER" />
		</user-service>
	</authentication-provider>
	
	
    
</beans:beans>
